{
  "name": "shift-complexity",
  "version": "0.9.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --out-dir dist src",
    "watch": "babel -w --out-dir dist src",
    "test": "mocha --inline-diffs --check-leaks --compilers js:babel-register --ui tdd --recursive test",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --report text --report html _mocha -- --inline-diffs --check-leaks --ui tdd --reporter dot --recursive test ",
    "lint": "eslint src test",
    "clean": "rm -rf dist/*",
    "browser": "browserify dist/index.js -s complexity -x shift-reducer > dist/shift-complexity.js",
    "browser-vendor": "browserify -r shift-reducer -r shift-parser > web-vendor.js",
    "release": "git-dirty && npm run clean && npm run lint && npm test && npm run build"
  },
  "author": "Jarrod Overson",
  "license": "MIT",
  "dependencies": {
    "shift-reducer": "^2.0.3"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^1.10.3",
    "git-dirty": "^1.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "shift-parser": "^3.0.4"
  }
}
