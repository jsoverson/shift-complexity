{
  "name": "shift-complexity",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --auxiliary-comment 'istanbul ignore next' --source-maps-inline --out-dir dist src --presets es2015",
    "build-browser": "npm run build && browserify -s complexity -o dist/shift-complexity.js dist/index.js",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "generate-coverage": "istanbul cover _mocha -- --inline-diffs --check-leaks --ui tdd --reporter dot --recursive test",
    "test": "mocha --inline-diffs --check-leaks --ui tdd --reporter dot --recursive test",
    "watch": "babel --auxiliary-comment 'istanbul ignore next' --source-maps-inline --out-dir dist src --watch --presets es2015",
    "watch-ts": "tsc -w --p src ",
    "clean": "rm -rf dist/*"
  },
  "author": "Shape Security",
  "license": "NONE",
  "dependencies": {
    "lodash": "^3.10.1",
    "shift-reducer": "^2.0.3"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.4",
    "shift-parser": "^3.0.4",
    "typescript": "^1.7.3"
  }
}
